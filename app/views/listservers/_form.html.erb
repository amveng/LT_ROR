<%= form_with model: @server, class: "nifty_form" do |f| %>

<div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control' %>
</div>

<div class="form-group">
    <%= f.label :urlServer %>
    <%= f.url_field :urlServer, class: 'form-control' %>
</div>

<div class="form-group">
    <%= f.label :serverversion_id %>
    <%= f.number_field :serverversion_id, class: 'form-control' %>
</div>

<div class="form-group">
    <%= f.label :dateStart %>
    <%= f.date_field :dateStart,  class: 'form-control' %>
</div>

<div class="form-group">
    <%= f.submit 'Сохранить', class: 'btn btn-primary' %>
</div>

<% end %>



<select name="city_id" id="city_id">
  <option value="1">Lisbon</option>
  <option value="2">Madrid</option>
  <option value="3">Berlin</option>
</select>

